@page
@model UniCare.Pages.ContinueModel
@{
} 
<div class="ulockd-inner-home" style=" background-image: url(../image/inner-pagebg.jpg);">
    <div class="container text-center">
        <div class="row">
            <div class="ulockd-inner-conraimer-details">
                <div class="col-lg-12">
                    <h1 class="text-uppercase">CONTACT US</h1>
                </div>
                <div class="col-lg-12">
                    <div class="ulockd-icd-layer">
                        <ul class="ulockd-icd-sub-menu">
                            <li class="list-inline-item"><a href="#"> HOME </a></li>
                            <li class="list-inline-item"><a href="#"> &gt; </a></li>
                            <li class="list-inline-item"> <a href="#"> CONTACT US </a> </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<section class="our-blog-home5 bgc-f9 pb70 ovh">
    <div class="container">
        <div class="row">

            <div class="col-xl-7 text-center">
                <div class="row">

                    <!--Start single blog post item-->
                    <div class="single-blog-post-item text-center">
                        <div class="blog-img-holder">
                            <img src="@Model.NewsBlog.Image" alt="">

                            <div class="blog-post-date">
                                <h1>
                                    @Model.NewsBlog.Date.ToString("dd")
                                    <br> <span>
                                        @Model.NewsBlog.Date.ToString("MMM")
                                    </span>
                                </h1>
                            </div>
                        </div>
                        <div class="blog-content-box">
                            <div class="single-bolg-title">
                                <h3>@Model.NewsBlog.Title</h3>

                            </div>
                            <div>
                                @Html.Raw(Model.NewsBlog.Message)
                            </div>
                        </div>
                    </div>



                </div> <!-- /.blog-content -->
            </div>
        </div>
    </div>
</section>
